node(){
    stage('Checkout'){
        checkout scm
    }
    stage('Printing Parameter'){
        sh "echo ${environment}" //comment
    }
    stage('Build'){
        sh "sh untilloop.sh"
    }
    stage('Array'){
        sh "sh array"
    }
    stage('Notify'){
        sh "mail -s 'The file ran successfully' aavasrimal@gmail.com'
    }
}